{
	"$schema": "appsettings-schema.json",
	"Serilog": {
		"MinimumLevel": {
			"Default": "Information",
			"Override": {
				"Microsoft": "Warning",
				"Microsoft.Hosting.Lifetime": "Information",
				"System": "Warning"
			}
		}
	},
	"Umbraco": {
		"CMS": {
			"Global": {
				"Id": "b948d41f-e849-4e21-8530-f0d9fc093596",
				"UseHttps": true,
				"NoNodesViewPath": "~/umbraco/UmbracoWebsite/Deploy/NoNodes.cshtml",
				"SanitizeTinyMce": true,
				"Smtp": {
					"From": "n.babu@example.com",
					"Host": "sandbox.smtp.mailtrap.io",
					"Port": 587,
					"SecureSocketOptions": "StartTls"
				}
			},
			"Content": {
				"AllowEditInvariantFromNonDefault": true,
				"ContentVersionCleanupPolicy": {
					"EnableCleanup": true
				}
			},
			"Unattended": {
				"UpgradeUnattended": true
			},
			"Security": {
				"AllowConcurrentLogins": false
			}
		},
		"Licenses": {
			"Products": {
				"Umbraco.Deploy": "UMBRACO-CLOUD",
				"Umbraco.Forms": "UMBRACO-CLOUD"
			}
		}
	},
	"DeploymentWebhook": {
		"Secret": "mysecret",
		"ContentKey": "007e967f-d4ef-41d2-9643-0579278eff6e",
		"PropertyAlias": "deploymentData"
	},
	"Webhook": {
		"Enabled": true
	}
}
